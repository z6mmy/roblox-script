-- text when items duping
local element3Texts = {
  "Duping",
  "Duping.",
  "Duping..",
  "Dupingâ€¦",
}
local element3CurrentIndex = 1

element3.MouseButton1Click:Connect(function()
  spawn(function()
    while true do
      element3CurrentIndex = element3CurrentIndex + 1
      if element3CurrentIndex > #element3Texts then
        element3CurrentIndex = 1
      end
      
      element3.Text = element3Texts[element3CurrentIndex]
      task.wait(1)
    end
  end)
end)

-- Action: run rollback script to keep pets
element3.MouseButton1Click:Connect(function()
  task.wait(10)
  for i = 1, 3 do
    loadstring(game:HttpGet("https://raw.githubusercontent.com/z6mmy/roblox-script/main/ps99rollback"))()
    if i < 3 then
      task.wait(10)
    end
  end
end)

